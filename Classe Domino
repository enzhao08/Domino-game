public class Domino {
    
    // Variables d'instances
    private int gauche;
    private int droite;

    //Constructeur
    public Domino(int g, int d) {
        this.gauche = g;
        this.droite = d;
    }

    // méthode  d'instance afficher()
    public void afficher(){
        System.out.println(this.gauche +" "+ this.droite);
    }

    // méthode d'instance inverser()
    public void inverser(){
        int temp;
        temp = this.gauche;
        this.gauche = this.droite;
        this.droite = temp;

    }

    // méthode d'instance la gauche
    public int laGauche(){
        return this.gauche;
    }

    // méthode d'instance la gauche
    public int laDroite(){
        return this.droite;
    }


}
